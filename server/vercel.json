{
    "builds":[
        {
            "src": "./app.js",
            "use" : "@vercel/node"
        }
    ],
    "routers": [
        {
            "src": "/.*",
            "dest": "server.js"
        }
    ],
    "rewrites": [
        { 
            "source": "/(.*)", 
            "destination": "/" 
        }
    ]

}